  <div class="container brand-campaign-stages">
    <div class="row">
      <div class="text-center">
        <%= link_to 'Contact Us', brands_agencies_path, :class => "btn btn-default btn-lg" %>
      </div>
    </div>
    <div class="row">
      <div class="container-social-icons text-center">
        <%= image_tag("vine_logo.svg", :class => "social-icons", :alt => "Vine Logo") %>
        <%= image_tag("twitter_logo.svg", :class => "social-icons", :alt => "Twitter Logo") %>
        <%= image_tag("instagram_logo.svg", :class => "social-icons", :alt => "Instagram Logo") %>
        <%= image_tag("facebook_logo.svg", :class => "social-icons", :alt => "Facebook Logo") %>
        <%= image_tag("pinterest_logo.svg", :class => "social-icons", :alt => "Pinterest Logo") %>
        <%= image_tag("snapchat_logo.svg", :class => "social-icons", :alt => "Snapchat Logo") %>
        <%= image_tag("tumblr_logo.svg", :class => "social-icons", :alt => "Tumblr Logo") %>
      </div>
    </div>
    <div class="row">
      <div class="campaign-stages text-center">
        <div class="container-stages-icons">
          <%= image_tag("pencil_icon.svg", :class => "stages-icons", :alt => "pencil icon") %>
