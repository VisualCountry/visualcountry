<div class="row contact-container text-center">
  <div class="col-md-6 col-md-offset-3">
    <form role="form">
      <div class="form-group">
        <label for="exampleInputEmail1">Name*</label>
        <input type="text" class="form-control" placeholder="Enter Name">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
      </div>
       <div class="form-group">
        <label for="exampleInputEmail1">Phone Number*</label>
        <input type="text" class="form-control" placeholder="Enter Phone Number">
      </div>
        <div class="form-group">
        <label for="exampleInputEmail1">Vine Account</label>
        <input type="text" class="form-control" placeholder="@vinehandle">
      </div>
        <div class="form-group">
        <label for="exampleInputEmail1">Instagram Account</label>
        <input type="text" class="form-control" placeholder="@instagramhandle">
