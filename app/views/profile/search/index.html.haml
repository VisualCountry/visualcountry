= render "layouts/search_nav_bar"
.search-users
  .profile-card
    - @profiles.each_slice(3) do |profile_group|
      .row
        - profile_group.each do |profile|
          %div{:class => "col-lg-4 col-md-4 col-sm-4 user-#{profile.id}"}
            = render 'shared/profile_card', user: profile
        %br/
